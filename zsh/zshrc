#!/bin/zsh

eval "$(zoxide init zsh)"

# +------------+
# | ENV         |
# +------------+

source $DOTFILES/zsh/zshenv

# +------------+
# | COMPLETION |
# +------------+
source $DOTFILES/zsh/completion.zsh

# +------------+
# | ALIASES    |
# +------------+
source $DOTFILES/aliases/aliases

# +------------+
# | FUNCTIONS  |
# +------------+
source $DOTFILES/zsh/functions.zsh

# +------------+
# | FUNCTIONS  |
# +------------+
source $DOTFILES/zsh/shortcuts.zsh
